  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "node ./internals/scripts/app-configure.js prod && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:jenkins": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p --progress",
    "build:qa": "node ./internals/scripts/app-configure.js qa && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p --progress",
    "build:dev": "node ./internals/scripts/app-configure.js dev && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p --progress",
    "build:clean": "rimraf ./build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "npm run start:dev",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:dev": "cross-env NODE_ENV=development node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env PORT=9000 NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|ts|tsx|graphql|mdx)\"",
    "prettier:format": "npm run prettier -- --write",
    "test": "is-ci \"test:cov\" \"test:watch\"",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:watchAll": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env NODE_ENV=test lint-staged"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
